{
    "title": "皇二甫学习笔记",
    "description": "一个线上笔记本",
    "author": "皇甫玉茹",
    "language": "zh-hans",
    "plugins": [
        "-search",
        "-highlight",
        "-lunr",
        "pageview-count",
        "popup",
        "search-plus",
        "anchor-navigation-ex",
        "splitter",
        "expandable-chapters",
        "chapter-fold",
        "github",
        "tbfed-pagefooter",
        "prism",
        "insert-logo"
    ],
    "pluginsConfig":{
        "anchor-navigation-ex": {
            "multipleH1": false
        },
        "github": {
            "url": "https://github.com/huangfuyuru"
        },
        "tbfed-pagefooter": {
            "copyright":"",
            "modify_label": "该文件最后修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "insert-logo": {
            "url": "./img/icon8.ico",
            "style": "background: none; max-height: 30px; min-height: 30px"
        },
        "favicon":{
            "shortcut": "./img/icon2.ico",
            "bookmark": "./img/icon2.ico",
            "appleTouch": "./img/icon2.ico",
            "appleTouchMore": {
                "120x120": "./img/icon2.ico",
                "180x180": "./img/icon2.ico"
            }
        }  
    }
}